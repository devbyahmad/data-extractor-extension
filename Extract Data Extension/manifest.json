{
  "manifest_version": 3,
  "name": "Email & Phone Number Extractor",
  "version": "1.0",
  "description": "Extract all emails and phone numbers from the current web page.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
